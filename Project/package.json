{
  "name": "vue-webpack-4",
  "version": "1.1.0",
  "description": "A Vuejs template using Webpack 4",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "prod": "webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "test:unit": "jest --config test/jest.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js",
    "cross": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "dev1": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "dev2": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "bundle-report":"webpack-bundle-analyzer --port 4200 dist/stats.json"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "Daniel Cook",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@testing-library/vue": "^4.2.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.5.4",
    "jest-serializer-vue": "^2.0.2",
    "lodash.template": "^4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.3.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^5.0.0",
    "uglify-es": "^3.3.9",
    "url-loader": "^3.0.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
    "vue-server-renderer": "^2.6.12",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
